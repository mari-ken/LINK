<h2>トークルーム作成</h2>
<%= form_with model: @room do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= attachment_image_tag @user, :image, :fill, 60, 60, fallback: "no_image.jpg" %>
  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :image, placeholder: "プロフィール画像" %>
  <%= f.submit %>
<% end %>
